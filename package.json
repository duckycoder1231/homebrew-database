{
  "name": "homebrew-database-server",
  "version": "0.1.0",
  "main": "server.js",
  "scripts": {
    "preinstall": "npm config set registry https://registry.npmjs.org/",
    "diagnose": "node -e \"console.log('npm registry:', require('child_process').execSync('npm config get registry').toString().trim())\"",
    "start": "node server.js",
    "reset-db": "node scripts/reset-db.js",
    "stop": "npx kill-port 3000"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "^4.22.1",
    "multer": "^2.0.2"
  }
}
